def process_numbers(numbers, operation):
    if operation == "double":
        return [n * 2 for n in numbers]
    elif operation == "square":
        return [n ** 2 for n in numbers]
    elif operation == "cube":
        return [n ** 3 for n in numbers]
    elif operation == "filter_even":
        return [n for n in numbers if n % 2 == 0]
    elif operation == "sum":
        return sum(numbers)
    else:
        print("invalid input")
        return None
